#!/usr/bin/env python3

import sys
import json
from pprint import pprint

def main():
  fileName = sys.argv[1:][0]
  with open(fileName) as jsonFile:
    data = json.load(jsonFile)

    instances = data["InstanceStatuses"]

    try:
      print(instances[0]["InstanceState"]["Name"])
    except IndexError:
      print("pending")

if __name__ == "__main__":
  main()
